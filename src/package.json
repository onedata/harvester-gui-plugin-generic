{
  "name": "harvester-gui-plugin-generic",
  "version": "1.1.0",
  "private": true,
  "description": "Default harvester GUI plugin for Onezone",
  "repository": "",
  "license": "MIT",
  "author": "ACK CYFRONET AGH",
  "scripts": {
    "build": "ember build --environment=production --output-path=../rel",
    "build:dev": "ember build --environment=development  --output-path=../rel",
    "build-plugin": "npm run build && cd .. &&  tar -czf plugin.tar.gz rel",
    "build-plugin:dev": "npm run build:dev && cd .. && tar -czf plugin.tar.gz rel",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "deps": "npm install",
    "clean": "rm -rf node_modules dist tmp ../rel ../plugin.tar.gz",
    "test": "ember test",
    "test:xvfb": "xvfb-run --server-args=\"-screen 0, 1366x768x24\" ember test",
    "test:xvfb-xunit-output": "xvfb-run --server-args=\"-screen 0, 1366x768x24\" ember test -r xunit",
    "lint": "npm run lint:js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@ember/optional-features": "2.0.0",
    "@ember/render-modifiers": "1.0.2",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@glimmer/component": "1.0.4",
    "@glimmer/tracking": "1.0.4",
    "abortcontroller-polyfill": "1.7.3",
    "bootstrap": "4.6.0",
    "broccoli-asset-rev": "3.0.0",
    "chai-dom": "1.9.0",
    "ember-auto-import": "1.12.0",
    "ember-bootstrap": "4.9.0",
    "ember-cli": "3.24.0",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-babel": "7.26.6",
    "ember-cli-chai": "0.5.0",
    "ember-cli-clipboard": "0.15.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-htmlbars": "5.7.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-moment-shim": "3.8.0",
    "ember-cli-sass": "10.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "2.0.2",
    "ember-cli-uglify": "3.0.0",
    "ember-data": "3.24.2",
    "ember-export-application-global": "2.0.1",
    "ember-fetch": "8.1.1",
