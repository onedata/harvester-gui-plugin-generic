{
  "name": "harvester-gui-plugin-generic",
  "version": "1.0.0",
  "private": true,
  "description": "Default harvester GUI plugin for Onezone",
  "repository": "",
  "license": "MIT",
  "author": "Michał Borzęcki",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production --output-path=../rel",
    "build:dev": "ember build --environment=development  --output-path=../rel",
    "build-plugin": "npm run build && cd .. &&  tar -czf plugin.tar.gz rel",
    "build-plugin:dev": "npm run build:dev && cd .. && tar -czf plugin.tar.gz rel",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "deps": "npm install --no-package-lock",
    "clean": "rm -rf node_modules dist tmp ../rel ../plugin.tar.gz",
    "test": "ember test",
    "test:xvfb": "xvfb-run --server-args=\"-screen 0, 1366x768x24\" ember test",
    "test:xvfb-xunit-output": "xvfb-run --server-args=\"-screen 0, 1366x768x24\" ember test -r xunit"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/render-modifiers": "1.0.2",
    "@fortawesome/fontawesome-free": "5.15.1",
    "@glimmer/component": "1.0.2",
    "@glimmer/tracking": "1.0.2",
    "abortcontroller-polyfill": "1.5.0",
    "babel-eslint": "10.1.0",
    "bootstrap": "4.5.3",
    "broccoli-asset-rev": "3.0.0",
    "chai-dom": "1.8.2",
    "ember-attacher": "1.1.1",
    "ember-auto-import": "1.6.0",
    "ember-await": "2.0.0",
    "ember-bootstrap": "4.3.0",
    "ember-cli": "3.20.2",
    "ember-cli-app-version": "4.0.0",
    "ember-cli-babel": "7.23.0",
    "ember-cli-chai": "0.5.0",
    "ember-cli-clipboard": "0.15.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars": "5.3.1",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-moment-shim": "3.8.0",
    "ember-cli-sass": "10.0.1",
    "ember-cli-spinjs": "2.1.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "2.0.2",
    "ember-cli-uglify": "3.0.0",
    "ember-data": "3.20.4",
    "ember-export-application-global": "2.0.1",
    "ember-fetch": "8.0.2",
    "ember-flatpickr": "3.0.1",
    "ember-intl": "5.5.0",
    "ember-load-initializers": "2.1.1",
    "ember-lodash": "4.19.5",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-mocha": "0.16.2",
    "ember-modifier": "2.1.1",
    "ember-moment": "8.0.0",
    "ember-power-select": "4.0.6",
    "ember-resolver": "8.0.2",
    "ember-simple-tree": "0.8.0",
    "ember-sinon": "5.0.0",
    "ember-source": "3.20.5",
    "eslint-plugin-ember": "9.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "loader.js": "4.7.0",
    "sass": "1.27.0",
    "sinon-chai": "3.5.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
}
